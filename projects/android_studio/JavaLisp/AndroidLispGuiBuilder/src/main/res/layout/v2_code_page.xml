<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id = "@+id/v2_result_fragment"
        android:layout_alignParentBottom="true">

    </FrameLayout>
    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/v2_result_fragment"
        android:id = "@+id/v2_inc_execute"
        layout = "@layout/v2_execute_pane">


    </include>
    <FrameLayout
        android:layout_width = "match_parent"
        android:layout_height="match_parent"
        android:id = "@+id/v2_frag_code_editor"
        android:layout_above="@id/v2_inc_execute"
        >

    </FrameLayout>



</RelativeLayout>